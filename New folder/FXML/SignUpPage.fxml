<AnchorPane id="AnchorPane" fx:id="signUpPane" prefHeight="800.0" prefWidth="1500.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FXML.SignUpPageController">
    <stylesheets>
        <URL value="@signuppage.css" />
    </stylesheets>
